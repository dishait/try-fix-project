{
  "version": "0.0.0",
  "name": "try-fix-project",
  "description": "尝试修复本地项目的依赖和版本问题",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dishait/try-fix-project.git"
  },
